{
  "builds": [
    { "src": "index.py", "use": "@vercel/python" },
    { "src": "build.sh", "use": "@vercel/static-build" }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.py" }
  ],
  "dependencies": {
    "pymongo": "^3.11.4"
  },
  "installCommand": "pip install pymongo, pymongo[srv], flask"
}
